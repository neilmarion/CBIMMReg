<table class="entries" id="members">
  <tr id="entries_header">
    <td>
      NAME
    </td>      
    <td>
     SCHOOL
    </td>
    <td>
      AREA OF SCHOOL
    </td>
    <td>
      YR-LVL
    </td>
    <td>
      COURSE / <br>SECTION
    </td>
    <td>
      LOCALE
    </td>
    <td>
     PRK-GRP
    </td>
    <td>
      KAPISANAN
    </td>
    <td>
      CONTACT NO
    </td>
    <td>

    </td>
  </tr>

  <% @members.each do |m| %>
    <tr class="entry">
      <td>
        <%= m.last_name %>, <%= m.first_name %>
      </td>
      <td>
        <%= m.school.name %>
      </td>
      <td>
        <%= m.area.name %>
      </td>
      <td>
        <%= Member::YEAR_LEVEL.invert[m.year_level] %>
      </td>
      <td>
        <%= m.course_section %>
      </td>
      <td>
        <%= m.locale.name %>
      </td>
      <td>
        <%= m.zone_group %>
      </td>
      <td>
        <%= m.circle.name %>
      </td>
      <td>
        <%= m.contact_number %>
      </td>
      <td>
        <%= button_to "delete", encoder_member_path(m), :method => :delete, :class => "header_link" %>
      </td>
    </tr>
  <% end %>





</table>

<div id="new" style="width:500px">
  <%= render 'new' %>
</div>



<% link_to "new member", new_encoder_member_path %>
